<% title("Edit a friendship") %>
<% subtitle("Here you can edit a friendship.") %> 
<% form_for(@friendship, :url => user_friendship_path, :method => "put") do |f| %>
  <%= f.error_messages %>
  <%= render :partial => "form", :object => f %>
  <%= f.submit "Update" %>
<% end %>
 
<br /><%= link_to "My friendships", user_friendship_path %>